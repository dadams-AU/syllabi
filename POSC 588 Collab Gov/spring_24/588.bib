@article{SCHNEIDER2009a,
  title = {Why Do {{Some Boundary Organizations Result}} in {{New Ideas}} and {{Practices}} and {{Others}} Only {{Meet Resistance}}?: {{Examples From Juvenile Justice}}},
  shorttitle = {Why Do {{Some Boundary Organizations Result}} in {{New Ideas}} and {{Practices}} and {{Others}} Only {{Meet Resistance}}?},
  author = {Schneider, Anne L.},
